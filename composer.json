{
    "name": "fridge/dbal-module",
    "description": "Provides a Fridge DBAL integration for your Zend Framework 2 project.",
    "keywords": [ "fridge", "dbal", "database", "pdo" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "fridge/dbal": "*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "FridgeDBALModule": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
